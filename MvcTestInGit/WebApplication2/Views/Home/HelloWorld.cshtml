
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@{ 
    ViewBag.Title = "HelloWorld";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>HelloWorld</title>
</head>
<body>
    <div> 
        <h2>@ViewBag.Title</h2>
       <ul>@for (int i = 0; i <ViewBag.NumTime; i++)
       {
        <li>@ViewBag.Message</li>
       }</ul> 
    </div>
</body>
</html>
