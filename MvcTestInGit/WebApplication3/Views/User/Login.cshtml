@model WebApplication3.Models.LoginModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>用户登录</title>
</head>
<body>
    <div>
        @using (Html.BeginForm())
        {
            {<div>用户名:</div> @Html.TextBoxFor(model => model.UserName)}
        <div>密码:</div>@Html.PasswordFor(model => model.Password)
        <div> <input type="submit" value="登陆" /></div>
           
        }

    </div>
</body>
</html>
